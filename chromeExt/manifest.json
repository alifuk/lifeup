{
  "manifest_version": 2,

  "name": "LifeUp",
  "description": "LifeUp extension used for adding links and quotes to your LifeUp",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",    
    "default_title": "Add link to LifeUp"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "cookies",
          "http://localhost/*"
  ]
}